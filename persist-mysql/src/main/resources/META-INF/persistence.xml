<persistence xmlns="http://java.sun.com/xml/ns/persistence"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
    version="1.0">
    <persistence-unit name="REMAN" transaction-type="RESOURCE_LOCAL">
        <provider>org.hibernate.ejb.HibernatePersistence</provider>
        <class>daomodels.County</class>       
        <class>daomodels.Food</class>   
        <class>daomodels.FoodType</class>   
        <class>daomodels.Reason</class>   
        <class>daomodels.Reservation</class>   
        <class>daomodels.ReservationPlaces</class>   
        <class>daomodels.Restaurant</class>   
        <class>daomodels.RestaurantFood</class>   
        <class>daomodels.RestaurantReason</class>   
        <class>daomodels.RestaurantTable</class>   
    </persistence-unit>
</persistence>